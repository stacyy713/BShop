{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
      },
    "extends": ["airbnb/base"],
    "rules": {
        "no-console": "off",
        "no-plusplus": "off",
        "eol-last": "off",
        "max-len": ["error", 150, { "ignoreUrls": true }],
        "no-tabs": "off",
        "no-unused-vars": "off",
        "no-param-reassign": "off",
        "linebreak-style": 0
      }
}